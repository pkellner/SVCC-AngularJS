  "browser": {
    "angular": "./node_modules/angular/angular.js",
    "angular-mocks": "./node_modules/angular-mocks/angular-mocks.js",
    "angular-messages": "./node_modules/angular-messages/angular-messages.js",
    "angular-scroll": "./node_modules/angular-scroll/angular-scroll.js",
    "ui.bootstrap": "./vendor/ui-bootstrap-tpls.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "ignore": "node_modules"
        }
      ],
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "angular": {
      "exports": "angular"
    },
    "angular-mocks": {
      "depends": [
        "angular"
      ]
    },
    "angular-messages": {
      "depends": [
        "angular"
      ],
      "exports": "angular.module('ngMessages').name"
    },
    "angular-scroll": {
      "depends": [
        "angular"
      ],
      "exports": "angular.module('duScroll').name"
    }
  }
}
